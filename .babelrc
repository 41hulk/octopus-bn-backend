{
    "presets": [
        "@babel/preset-env"
    ],
    "plugins": [
        ["module-resolver", {
          "root": ["./src"],
          "alias": {
            "test": "./test"
          }
        }],
        [
          "@babel/plugin-transform-runtime",
      {
          "absoluteRuntime": true,
          "corejs": false,
          "helpers": true,
          "regenerator": true,
          "useESModules": false
      }
      ]
      ]
}
